package cn.bjsxt.collection;

public class MyLinkedList /*implements List*/{
	private Node first;
	private Node last;
	
	private int size;
	
	public static void main(String[] args){
		MyLinkedList list = new MyLinkedList();
		list.add("aaa");
		list.add("abb");
		System.out.println(list.size());
		System.out.println(list.get(1));
	}

	private void add(Object obj) {
		Node n = new Node();
		if(first == null){
			n.setPrevious(null);
			n.setObj(obj);
			n.setNext(null);
			
			first = n;
			last = n;
		}else{
			//直接往last节点后增加新的节点
			n.setPrevious(last);
			n.setObj(obj);
			n.setNext(null);
			
			last.setNext(n);
			
			last = n;
		}
		size++;
	}
	
	public Object get(int index){
		Node temp = null;
		if(first != null){
			temp = first;
			for(int i = 0;i < index;i++)
				temp = temp.next;
		}
		
		//index越界怎么处理？
		
		return temp.obj;
	}
	public int size(){
		return size;
	}
	
}

